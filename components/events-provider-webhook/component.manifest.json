{
  "$schema": "https://greentic-ai.github.io/greentic-component/schemas/v1/component.manifest.schema.json",
  "artifacts": {
    "component_wasm": "target/wasm32-wasip2/release/events_provider_webhook.wasm"
  },
  "capabilities": {
    "host": {
      "messaging": {
        "inbound": true,
        "outbound": true
      },
      "http": {
        "client": true,
        "server": false
      },
      "secrets": {
        "required": []
      },
      "telemetry": {
        "scope": "node"
      }
    },
    "wasi": {
      "clocks": true,
      "filesystem": {
        "mode": "none",
        "mounts": []
      },
      "random": true
    }
  },
  "config_schema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "properties": {
      "auth": {
        "type": "string"
      },
      "headers": {
        "additionalProperties": {
          "type": "string"
        },
        "type": "object"
      },
      "method": {
        "default": "POST",
        "type": "string"
      },
      "target_url": {
        "minLength": 1,
        "type": "string"
      },
      "timeout_ms": {
        "minimum": 1,
        "type": "integer"
      }
    },
    "required": [
      "target_url"
    ],
    "title": "events-provider-webhook config",
    "type": "object"
  },
  "default_operation": "ingest_http",
  "describe_export": "get-manifest",
  "dev_flows": {
    "custom": {
      "format": "flow-ir-json",
      "graph": {
        "description": "Auto-generated custom config for ai.greentic.events-provider-webhook",
        "id": "ai.greentic.events-provider-webhook.custom",
        "kind": "component-config",
        "nodes": {
          "ask_config": {
            "questions": {
              "fields": [
                {
                  "default": {
                    "headers": {},
                    "method": "POST",
                    "target_url": "https://example.invalid/webhook"
                  },
                  "id": "config",
                  "prompt": "Config:",
                  "type": "string"
                },
                {
                  "id": "correlation_id",
                  "prompt": "Correlation Id:",
                  "type": "string"
                },
                {
                  "default": {},
                  "id": "event",
                  "prompt": "Event:",
                  "type": "string"
                },
                {
                  "id": "handler_id",
                  "prompt": "Handler Id:",
                  "type": "string"
                },
                {
                  "id": "http",
                  "prompt": "Http:",
                  "type": "string"
                },
                {
                  "id": "raw",
                  "prompt": "Raw:",
                  "type": "string"
                },
                {
                  "id": "team",
                  "prompt": "Team:",
                  "type": "string"
                },
                {
                  "id": "tenant",
                  "prompt": "Tenant:",
                  "type": "string"
                }
              ]
            },
            "routing": [
              {
                "to": "emit_config"
              }
            ]
          },
          "emit_config": {
            "template": "{\n  \"node_id\": \"events-provider-webhook\",\n  \"node\": {\n    \"ingest_http\": {\n      \"input\": {\n        \"config\": \"{{state.config}}\",\n        \"correlation_id\": \"{{state.correlation_id}}\",\n        \"event\": \"{{state.event}}\",\n        \"handler_id\": \"{{state.handler_id}}\",\n        \"http\": \"{{state.http}}\",\n        \"raw\": \"{{state.raw}}\",\n        \"team\": \"{{state.team}}\",\n        \"tenant\": \"{{state.tenant}}\"\n      }\n    },\n    \"routing\": [\n      { \"to\": \"NEXT_NODE_PLACEHOLDER\" }\n    ]\n  }\n}"
          }
        }
      }
    },
    "default": {
      "format": "flow-ir-json",
      "graph": {
        "description": "Auto-generated default config for ai.greentic.events-provider-webhook",
        "id": "ai.greentic.events-provider-webhook.default",
        "kind": "component-config",
        "nodes": {
          "emit_config": {
            "template": "{\n  \"node_id\": \"events-provider-webhook\",\n  \"node\": {\n    \"ingest_http\": {\n      \"input\": {\n        \"config\": {\"headers\":{},\"method\":\"POST\",\"target_url\":\"https://example.invalid/webhook\"},\n        \"event\": {}\n      }\n    },\n    \"routing\": [\n      { \"to\": \"NEXT_NODE_PLACEHOLDER\" }\n    ]\n  }\n}"
          }
        }
      }
    }
  },
  "hashes": {
    "component_wasm": "blake3:0c9fb84c27d8bb21a3c54d3c9edecdcd7f96452993183b92a221c9d3742a54c9"
  },
  "id": "ai.greentic.events-provider-webhook",
  "limits": {
    "memory_mb": 128,
    "wall_time_ms": 1000
  },
  "name": "events-provider-webhook",
  "operations": [
    {
      "input_schema": {
        "$ref": "schemas/io/input.schema.json"
      },
      "name": "ingest_http",
      "output_schema": {
        "$ref": "schemas/io/output.schema.json"
      }
    },
    {
      "input_schema": {
        "$ref": "schemas/io/input.schema.json"
      },
      "name": "publish",
      "output_schema": {
        "$ref": "schemas/io/output.schema.json"
      }
    }
  ],
  "profiles": {
    "default": "stateless",
    "supported": [
      "stateless"
    ]
  },
  "secret_requirements": [],
  "supports": [
    "messaging"
  ],
  "version": "0.1.0",
  "world": "greentic:component/component@0.6.0"
}
