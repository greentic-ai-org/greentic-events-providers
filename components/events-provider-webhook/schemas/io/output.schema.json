{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "events-provider-webhook output",
  "type": "object",
  "required": ["receipt_id", "status", "emitted_events"],
  "properties": {
    "receipt_id": { "type": "string" },
    "status": { "type": "string" },
    "dispatched": { "type": "boolean" },
    "state_key": { "type": "string" },
    "request": { "type": "object" },
    "response": { "type": "object" },
    "emitted_events": {
      "type": "array",
      "items": { "type": "object" }
    }
  },
  "additionalProperties": true
}
