{
  "$schema": "https://greentic-ai.github.io/greentic-component/schemas/v1/component.manifest.schema.json",
  "artifacts": {
    "component_wasm": "target/wasm32-wasip2/release/events_provider_timer.wasm"
  },
  "capabilities": {
    "host": {
      "messaging": {
        "inbound": true,
        "outbound": true
      },
      "secrets": {
        "required": []
      },
      "telemetry": {
        "scope": "node"
      }
    },
    "wasi": {
      "clocks": true,
      "filesystem": {
        "mode": "none",
        "mounts": []
      },
      "random": true
    }
  },
  "config_schema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "properties": {
      "default_delay_seconds": {
        "minimum": 0,
        "type": "integer"
      },
      "persistence_key_prefix": {
        "type": "string"
      },
      "timezone": {
        "default": "UTC",
        "type": "string"
      }
    },
    "title": "events-provider-timer config",
    "type": "object"
  },
  "default_operation": "timer_tick",
  "describe_export": "get-manifest",
  "dev_flows": {
    "custom": {
      "format": "flow-ir-json",
      "graph": {
        "description": "Auto-generated custom config for ai.greentic.events-provider-timer",
        "id": "ai.greentic.events-provider-timer.custom",
        "kind": "component-config",
        "nodes": {
          "ask_config": {
            "questions": {
              "fields": [
                {
                  "default": {
                    "timezone": "UTC"
                  },
                  "id": "config",
                  "prompt": "Config:",
                  "type": "string"
                },
                {
                  "id": "correlation_id",
                  "prompt": "Correlation Id:",
                  "type": "string"
                },
                {
                  "default": {},
                  "id": "event",
                  "prompt": "Event:",
                  "type": "string"
                },
                {
                  "id": "handler_id",
                  "prompt": "Handler Id:",
                  "type": "string"
                },
                {
                  "id": "team",
                  "prompt": "Team:",
                  "type": "string"
                },
                {
                  "id": "tenant",
                  "prompt": "Tenant:",
                  "type": "string"
                }
              ]
            },
            "routing": [
              {
                "to": "emit_config"
              }
            ]
          },
          "emit_config": {
            "template": "{\n  \"node_id\": \"events-provider-timer\",\n  \"node\": {\n    \"timer_tick\": {\n      \"input\": {\n        \"config\": \"{{state.config}}\",\n        \"correlation_id\": \"{{state.correlation_id}}\",\n        \"event\": \"{{state.event}}\",\n        \"handler_id\": \"{{state.handler_id}}\",\n        \"team\": \"{{state.team}}\",\n        \"tenant\": \"{{state.tenant}}\"\n      }\n    },\n    \"routing\": [\n      { \"to\": \"NEXT_NODE_PLACEHOLDER\" }\n    ]\n  }\n}"
          }
        }
      }
    },
    "default": {
      "format": "flow-ir-json",
      "graph": {
        "description": "Auto-generated default config for ai.greentic.events-provider-timer",
        "id": "ai.greentic.events-provider-timer.default",
        "kind": "component-config",
        "nodes": {
          "emit_config": {
            "template": "{\n  \"node_id\": \"events-provider-timer\",\n  \"node\": {\n    \"timer_tick\": {\n      \"input\": {\n        \"config\": {\"timezone\":\"UTC\"},\n        \"event\": {}\n      }\n    },\n    \"routing\": [\n      { \"to\": \"NEXT_NODE_PLACEHOLDER\" }\n    ]\n  }\n}"
          }
        }
      }
    }
  },
  "hashes": {
    "component_wasm": "blake3:7e86df0db927948e390e262931aa1f4e62ed2496e08991d593d4a80a70033de2"
  },
  "id": "ai.greentic.events-provider-timer",
  "limits": {
    "memory_mb": 128,
    "wall_time_ms": 1000
  },
  "name": "events-provider-timer",
  "operations": [
    {
      "input_schema": {
        "$ref": "schemas/io/input.schema.json"
      },
      "name": "timer_tick",
      "output_schema": {
        "$ref": "schemas/io/output.schema.json"
      }
    },
    {
      "input_schema": {
        "$ref": "schemas/io/input.schema.json"
      },
      "name": "publish",
      "output_schema": {
        "$ref": "schemas/io/output.schema.json"
      }
    }
  ],
  "profiles": {
    "default": "stateless",
    "supported": [
      "stateless"
    ]
  },
  "secret_requirements": [],
  "supports": [
    "messaging"
  ],
  "version": "0.1.0",
  "world": "greentic:component/component@0.6.0"
}
